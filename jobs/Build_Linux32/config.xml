          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>LINUX_PACKAGE_DIR</name>
          <description>Specify the directory where linux package to be copied</description>
          <defaultValue></defaultValue>
