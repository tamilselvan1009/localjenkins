export VECTORCAST_DIR=/mnt/nfs/shared/.VCAST_REGRESSION/VC_latest/linux
export VECTOR_LICENSE_FILE=/home/linuxregressionmac/flexlm/vector-UBUNTU.lic
export PYTHONPATH=/usr/lib/python2.7/dist-packages:PYTHONPATH
cd $WORKSPACE
cp -r * /mnt/nfs/shared/Git_Latest/.

./build.sh
cd build
./install.sh || if [[ $? == 1 ]]; then 
		true
    else
